@model IEnumerable<PA2_2022_2C_WebAppMVC.Models.Provincias>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Nombre Provincia
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomProvincia)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Provincia">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Provincia">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Provincia">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
